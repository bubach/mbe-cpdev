<script type="text/javascript">
	function switchState(selectedElement, stateElementName ) {
		
		var stateElement = document.getElementById(stateElementName);
		if (selectedElement.value == 'US') {
			
			stateElement.style.display = '';
			//$(stateElementName).show();
		} else {
			//$(stateElementName).hide();
			stateElement.style.display = 'none';
		};
	}

	var allok = false;
	var firstchecked = '';

	function beforeSubmit() {
		allok = true;
		firstchecked = '';

		checkElementString('business_name');
		checkElementString('address_primary');
		checkElementString('city');
		checkElementString('country');
		if (document.getElementById('country').value == 'United States Of America') {
			checkElementString('state');
		};
		checkElementString('phone');
		checkElementString('applicants_email');

		if (allok == false) {
			document.getElementById(firstchecked).focus();
			return;
		};

		document.forms['credit_application_form'].submit();
	}

	function checkElementString(elementId) {
		var element = document.getElementById(elementId);
		element.style.borderBottom = '1px solid #ccc';
		if (element.value.trim() == '') {
			element.style.borderBottom = '1px solid red';
			allok = false;
			if (firstchecked == '') {
				firstchecked = elementId;
			};
		};
	}

</script>




